<% if (title) { %>
<h1><%= title %></h1>
<% } else {
  res.redirect('/')
} %>
<%- messages() %>
<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>
<div class="classification-container">
  <form action="./deleteinventory" method="post">
      <div class="form-group">
          <label for="inv_id">Inventory</label>
          <%- selectionInventory %>
        </div>
    </div>
    <div class="form-group">
    <input type="submit" value="Delete Inventory" />
    </div>
  </form>
</div>